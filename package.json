{
  "private": true,
  "name": "unban-message",
  "version": "0.0.0",
  "license": "BSD-3-Clause",
  "type": "module",
  "scripts": {
    "build:server": "cd src/server && vite build",
    "build": "npm run build:server",
    "deploy": "npm run build && devvit upload",
    "dev": "concurrently -k -p \"[{name}]\" -n \"SERVER,DEVVIT\" -c \"blue,green,magenta\" \"npm run dev:server\" \"npm run dev:devvit\"",
    "dev:devvit": "dotenv -e .env -- devvit playtest",
    "dev:server": "cd src/server && vite build --watch",
    "login": "devvit login",
    "launch": "npm run build && npm run deploy && devvit publish",
    "type-check": "tsc --build"
  },
  "dependencies": {
    "@devvit/web": "0.12.5",
    "express": "5.1.0"
  },
  "devDependencies": {
    "@types/express": "5.0.1",
    "concurrently": "9.1.2",
    "devvit": "0.12.5",
    "dotenv-cli": "8.0.0",
    "typescript": "5.8.3",
    "vite": "^7.2.6"
  }
}